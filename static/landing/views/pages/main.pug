extends ../index

block main-content
  div.main-page-wrapper
    iframe(src="//vk.com/video_ext.php?oid=-958593&id=456239570&hash=cb2fe5b67b42503d&hd=2" width="853" height="480" frameborder="0" allowfullscreen).video-iframne

    if eventData
      div.main-page-block
        h2
          | Ближайшее событие
        include ../partials/event
    if newsData && newsData.length
      div.main-page-block
        h2
          | Недавние новости
        include ../partials/news-list

    a(href="#polozhenie_o_festivale")
    include ../documents/polozhenie-fest-20
    a(href="#printsipy_raboty_istorichekoy_komissii")
    include ../documents/printsipy-raboty-istorichekoy-komissii.pug
    a(href="#event_request_manual")
    include ../documents/event-request-manual.pug

block after-index-script
  script(src='/dist/js/landing-main.js?timestamp=?timestamp=' + appStartTime)